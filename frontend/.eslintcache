ead>\r\n                    <tbody>\r\n                        {users.map(user => (\r\n                            <tr key={user._id} className=\"table-warning\">\r\n                                <td>{user._id}</td>\r\n                                <td>{user.name}</td>\r\n                                <td>{user.email}</td>\r\n                                <td>{user.isAdmin ? <i className='fas fa-check' style={{color: 'blue'}}></i> : (\r\n                                     <i className='fas fa-times' style={{color: 'red'}}></i>\r\n                                )}</td>\r\n                                <td>\r\n                                    <LinkContainer to={`user/${user._id}/edit`}>\r\n                                        <Button variant='light' className='btn-sm'>\r\n                                             <i className='fas fa-edit'></i>\r\n                                        </Button>\r\n                                    </LinkContainer>\r\n                                    <Button variant=\"danger\" className=\"btn-sm\" onClick={() => \r\n                                    deleteHandler(user._id)}>\r\n                                        <i className=\"fas fa-trash\"></i>\r\n                                    </Button>\r\n                                </td>\r\n                            </tr>\r\n                        ))}\r\n                    </tbody>\r\n                </Table>\r\n            )}\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default AdminUserList;\r\n","C:\\Users\\imank\\Desktop\\FYP\\CyberShop\\frontend\\src\\components\\layouts\\AdminUserEdit.js",["209","210"],"import React, { useState, useEffect, Fragment } from 'react';\r\nimport FormContainer from '../layouts/contents/FormContainer';\r\nimport { Form, Button, Row, Col } from 'react-bootstrap';\r\nimport { Link } from 'react-router-dom';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\nimport { getUserDetails, updateUser } from '../../